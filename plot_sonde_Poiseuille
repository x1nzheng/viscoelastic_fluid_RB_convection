set multiplot layout 3,1 rowsfirst
unset key 

set title 'u_1(x2=.5)'
plot 'sonde_Poiseuille' using 2:4 w l linetype 3, 'sonde_Poiseuille' using 2:3 w l linetype 1

set title 'tau_11(x2=1)'
plot 'sonde_Poiseuille' using 2:5 w l linetype 3, 'sonde_Poiseuille' using 2:6 w l linetype 1

set title 'tau_12(x2=1)'
plot 'sonde_Poiseuille' using 2:7 w l linetype 3, 'sonde_Poiseuille' using 2:8 w l linetype 1


pause 1
reread


 










