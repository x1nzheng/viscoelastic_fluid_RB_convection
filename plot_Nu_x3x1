reset

set border linecolor rgbcolor "yellow"
set key textcolor rgbcolor "yellow"
set title "Test Plot" textcolor rgbcolor "yellow"
set obj 1 rectangle behind from screen 0,0 to screen 1,1
set obj 1 fillstyle solid 1.0 fillcolor rgbcolor "black"



 set datafile missing '?' 

 set pm3d # map 
 set surface
 
 
unset key  
#set colorbox
 
set palette rgbformulae 23,28,3
 
set size ratio 1.0 

set style fill transparent solid 0.65

set title  'Nu_local' 
set xlabel 'x_1' tc ls 2
set ylabel 'x_3' tc ls 2
set zlabel 'Nu_local' tc ls 2

splot "Nu_local_x3x1_paroi_bas" using 1:2:3 with lines lc 'black' 
