set border linecolor rgbcolor "blue"
set key textcolor rgbcolor "blue"
set title "Test Plot" textcolor rgbcolor "red"
set obj 1 rectangle behind from screen 0,0 to screen 1,1
set obj 1 fillstyle solid 1. fillcolor rgbcolor "black"



 set datafile missing '?' 

set pm3d map 
set contour base
set cntrparam levels 50 #10
set surface

set style increment user
do for [i=1:51] {
set style line i lc rgb "yellow" lw .5 
#set style line i lc rgb "black" lw .8      
}

 
  set clabel '%8.3g'  

set cntrparam bspline
#unset interpolate 0,0 


#set key on outside  t #rm 
unset key  
#unset colorbox
set colorbox
#unset contour
unset surface 	
#set palette rgbformulae 33,13,10
set palette rgbformulae 23,28,3
 
#set size ratio .25
set zlabel  offset character 1, 0, 0 font "" textcolor lt 3 norotate
       

set title 'u_1'
splot "champs_UT" using 1:2:3  lc 'black' #, "champs_UT" using 1:2:3 with labels boxed
